{{!-- <div class="column justify-content-center"></div> --}}
<form action="/auth/login" method="POST" id="form-container" class="login">
  <div class="form-group">
  <label for="username">Username</label>
  <input id="username" type="text" name="username" placeholder="Your Username" class="form-control">
  </div>
  <div class="form-group">
  <label for="password">Password</label>
  <input id="password" type="password" name="password" placeholder="Your password" class="form-control">
  <div class="form-group">
    {{#if message }}
  <button type="submit" class="btn btn-dark">Login</button>
  <div class="error-message">{{ message }}</div>
  {{/if}}
  </div>
  <div class="form-group"></div>
  <button type="submit" class="but form-control">Login</button>
  </div>
  <br>
  {{!-- <br>
  <label for="email">Email</label>
  <input id="email" type="email" name="email" placeholder="Your Email"> --}}
  <p class="account-message">
    Don't have an account yet?
    <a href="/auth/signup">Signup</a>
  </p>
  </form>
  <div class="icons">
    <img src="/images/parkImages/banco.png" alt="icon">
